{
  "name": "react-isomorphic-hot-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [
    "react",
    "react-router",
    "isomorphic",
    "universal",
    "react-hot-loader",
    "redux",
    "webpack",
    "koa",
    "postcss"
  ],
  "scripts": {
    "start": "node --harmony ./development/client.js",
    "build-server": "webpack --display-error-details --config ./config/webpack.server.js",
    "build-client": "webpack --display-error-details --config ./config/webpack.client.js",
    "build": "npm run build-client && npm run build-server",
    "clean": "rm -rf ./dist/* && rm -rf ./static/dist/*",
    "linc": "git diff --name-only --diff-filter=ACMRTUB `git merge-base HEAD master` | grep '\\.js$'"
  },
